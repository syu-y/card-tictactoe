{
  "name": "card-tictactoe",
  "version": "0.0.1",
  "description": "Strategic Tic-Tac-Toe game with card mechanics",
  "private": true,
  "scripts": {
    "dev": "vite dev --host",
    "dev:server": "vite build && node dev-server.js",
    "devstart": "node dev-server.js",
    "build": "vite build",
    "start": "node dev-server.js",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "vitest",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write ."
  },
  "devDependencies": {

    "@sveltejs/adapter-node": "^5.4.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^5.1.1",
    "@types/node": "^20.10.0",
    "@types/ws": "^8.5.10",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.35.1",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.1.2",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^7.3.0",
    "vitest": "^4.0.16"
  },
  "type": "module",
  "dependencies": {
    "express": "^4.18.2",
    "tsx": "^4.21.0",
    "ws": "^8.5.0"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
